{
  "name": "madabrand-website",
  "version": "1.0.0",
  "description": "MadaBrand - Premium Design Studio Website",
  "main": "index.html",
  "homepage": "https://madabrand.vercel.app",
  "scripts": {
    "dev": "tailwindcss -i ./styles/style.css -o ./dist/output.css --watch",
    "build": "tailwindcss -i ./styles/style.css -o ./dist/output.css --minify",
    "build:vercel": "npm run build",
    "start": "live-server .",
    "predeploy": "npm run build",
    "deploy": "echo 'Deploy to Vercel via dashboard or CLI'",
    "fix-paths": "node fix-paths.js",
    "prebuild": "node pre-build.js",
    "test": "echo 'No tests yet'"
  },
  "keywords": [
    "branding",
    "design",
    "portfolio",
    "website"
  ],
  "author": "MadaBrand",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hendrix-D-Dev/madabrand.git"
  },
  "bugs": {
    "url": "https://github.com/Hendrix-D-Dev/madabrand/issues"
  }
}
